#Hydrogeology{
    *{
        padding: 0;
        margin: 0;
    }
    height: 100%;
    aside{
        min-width: 220px!important;
    }
    .barrierList{
        height: 300%;
        width: 220px;
        padding: 0 10px;
        background-color: #272b33;
        color: #fff;
        .barrier_List_Title{
            height: 46px;
            line-height: 46px;
            border-bottom: 1px solid #ccc;
        }
        ul{
            li{
                padding: 6px;
                border-radius: 4px;
                color: #fff;
            }
            li:nth-child(2n){            
                background-color: rgb(47,53,65);            
            }
        }
    }

    Header{
        padding-right: 0!important;
        position: fixed;
        width: 100%;
        top: 50px;
        z-index: 999;
        .head{
            .titleLeft{
                height: 100%;
                .threeBtn:hover{
                    box-shadow: 0 2px 2px 2px #ccc;
                }
                div{
                    width: 40px;
                    height: 64px;
                    display: flex;
                    float: left;
                    text-align: center;
                    padding-top: 10px;
                    margin: 0 10px;
                    padding-left: 4px;
                    padding-right: 4px;
                    flex-direction: column;
                    justify-content: space-around;
                    img{
                        width: 100%;
                        height: 24px;
                    }
                    p{
                        margin: 0;
                        padding: 0;
                        width: 100%;
                        height: 34px;
                        line-height: 34px;
                        font-size: 12px;
                        color: #fff;
                    }
                }
            }
        }
        
    }
}